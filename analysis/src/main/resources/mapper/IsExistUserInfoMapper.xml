<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jlysh.analysis.pojo.mapper.IsExistUserInfoMapper">
    <select id="isExistUserInfo" resultType="java.lang.Boolean" >
        SELECT COUNT(num)
        FROM sys_user
        WHERE num=#{num}
    </select>

</mapper>
